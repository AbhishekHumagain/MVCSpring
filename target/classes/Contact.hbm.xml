<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="myapp.model.Contact" table="employeecontact">
        <id name="employeeID" type="string" column="employeeID">
            <generator class="assigned" />
           
        </id>
        
        <property name="employeeName" type="string" column="employeeName" not-null="true" />
        <property name="employeeNo" type="string" column="employeeNo" not-null="true" />
        <property name="homeNo" type="string" column="homeNo"  not-null="true" />
        <property name="officeNo" type="string" column="officeNo"  not-null="true" />
        <property name="email" type="string" column="email"  not-null="true" />
    </class>
   
</hibernate-mapping>
